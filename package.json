    "@atproto/identity": "^0.4.3",
    "@atproto/lexicon": "^0.4.4",
    "@atproto/oauth-client-node": "^0.2.5",
    "@atproto/repo": "^0.6.0",
    "@atproto/sync": "^0.1.7",
    "@atproto/syntax": "^0.3.1",
    "@atproto/xrpc-server": "^0.7.4",
    "better-sqlite3": "^11.7.0",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "iron-session": "^8.0.4",
    "kysely": "^0.27.5",
    "multiformats": "^9.9.0",
    "uhtml": "^4.7.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "inquirer": "^12.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
